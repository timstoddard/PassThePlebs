{
  "name": "pass-the-plebs",
  "version": "1.1.4",
  "description": "Pass the Plebs (Chrome extension for pass.calpoly.edu)",
  "author": "Tim Stoddard",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/timstoddard/PassThePlebs"
  },
  "main": "src/extension/js/index.js",
  "scripts": {
    "start": "onchange 'src/**/*' -w -- npm run dev",
    "dev": "npm run build && npm run reload && zip -r dist.zip dist",
    "build": "rimraf dist* && npm run pack-files",
    "pack-files": "webpack -p --config webpack/config.js && npm run move-files",
    "move-files": "cp src/manifest.json dist && mkdir dist/icons && cp media/icons/*.png dist/icons",
    "reload": "chrome-cli open chrome://extensions && chrome-cli reload && chrome-cli close && chrome-cli reload",
    "lint": "eslint --ext .js --ignore-pattern '/dist/*' --fix .",
    "postinstall": "brew install chrome-cli && brew upgrade chrome-cli"
  },
  "dependencies": {
    "url-regex": "5.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "autoprefixer": "9.5.1",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.6",
    "css-loader": "2.1.1",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.17.2",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "jquery": "3.4.1",
    "mini-css-extract-plugin": "0.6.0",
    "node-sass": "4.12.0",
    "onchange": "6.0.0",
    "postcss-loader": "3.0.0",
    "rimraf": "2.6.3",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "uglifyjs-webpack-plugin": "2.1.3",
    "webpack": "4.31.0",
    "webpack-cli": "3.3.2",
    "webpack-merge": "4.2.1"
  }
}
